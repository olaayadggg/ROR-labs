<p style="color: green"><%= notice %></p>

<h1>Tracks</h1>

<div id="tracks">
  <% @tracks.each do |track| %>
    <%= render track %>
    <p class="btn btn-primary">
      <%= link_to "Show this track", track %>
    </p>
  <% end %>
</div>
<p class="btn btn-primary">
<%= link_to "New track", new_track_path %>
</p>
